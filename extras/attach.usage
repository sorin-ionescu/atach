attach [(--help|-h) | (--list|-l) | (--sockets|-L) | (--version|-v)];
attach <opts> <session>;
attach <opts> <command>; 
session = !attach -l;
opts = [(--session|-s) <session> | (--char|-c) <char> | (--redraw|-r) <modes> | (--detached|-d) | (--no-detach|-D) | (--no-suspend|-Z)] ...;
modes = (none | ctrl_l | winch);
